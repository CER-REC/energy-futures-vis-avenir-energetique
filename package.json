{
  "name": "d3-neb",
  "version": "1.0.13.4",
  "description": "NEB project",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "browserify JS/App.coffee -t coffeeify --extension='.coffee' -t [ stringify --extensions [.mustache .css] ] -g uglifyify -o dist/public/bundle.js",
    "watch": "watchify JS/App.coffee -t coffeeify --extension='.coffee' -t [ stringify --extensions [.mustache .css] ] -o public/bundle.js --debug --verbose",
    "distribute": "node distribute.js",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "archiver": "^1.2.0",
    "array-includes": "^3.0.2",
    "browserify": "^13.1.1",
    "coffeeify": "^2.0.1",
    "d3": "^3.5.17",
    "debug": "~2.3.0",
    "domready": "^1.0.8",
    "grunt": "^1.0.1",
    "lodash": "^4.16.6",
    "mustache": "^2.2.1",
    "query-string": "^4.2.3",
    "stringify": "^5.1.0",
    "watchify": "^3.7.0"
  },
  "author": "Stephanie Sachrajda",
  "devDependencies": {
    "convert-source-map": "^1.3.0",
    "express": "^4.14.0",
    "fs-extra": "^1.0.0",
    "markdown-styles": "^3.1.8",
    "mustache-express": "^1.2.2",
    "uglifyify": "^3.0.4"
  },
