{
  "name": "d3-neb",
  "version": "1.1.6-1",
  "description": "NEB project",
  "scripts": {
    "build": "browserify JS/App.coffee -p ./tasks/prod-dotenv-browserify-plugin.js -t coffeeify --extension='.coffee' -t [ stringify --extensions [.mustache .css] ] -t envify -g uglifyify -o dist/public/bundle.js",
    "watch": "watchify JS/App.coffee -p ./tasks/dev-dotenv-browserify-plugin.js -t coffeeify --extension='.coffee' -t [ stringify --extensions [.mustache .css] ] -t envify -o public/bundle.js --debug --verbose",
    "distribute": "node ./tasks/distribute.js",
    "start": "node ./JS/servers/DevelopmentServer/DevelopmentServer.js",
    "start-production": "node ./JS/servers/ProductionServer/ProductionServer.js",
    "ingest": "node ./JS/validation/Ingest.js",
    "test": "mocha --recursive JS/test --compilers coffee:coffee-script/register",
    "test-watch": "mocha --recursive JS/test --compilers coffee:coffee-script/register --watch test",
    "visual-studio-install": "node ./tasks/visual-studio-install.js",
    "clean-vs": "node tasks/clean-vs.js"
  },
