{
  "name": "d3-neb",
  "version": "1.0.13.4",
  "description": "NEB project",
  "scripts": {
    "build": "browserify JS/App.coffee -p ./tasks/dotenv-browserify-plugin.js -t coffeeify --extension='.coffee' -t [ stringify --extensions [.mustache .css] -t envify -g uglifyify -o dist/public/bundle.js",
    "watch": "watchify JS/App.coffee -p ./tasks/dotenv-browserify-plugin.js -t coffeeify --extension='.coffee' -t [ stringify --extensions [.mustache .css] ] -t envify -o public/bundle.js --debug --verbose",
    "distribute": "node ./tasks/distribute.js",
    "start": "node ./JS/servers/DevelopmentServer/DevelopmentServer.js",
    "start-image-server": "node ./JS/servers/ImageServer/ImageServer.js",
    "ingest": "node ./JS/validation/Ingest.js",
    "test": "mocha --recursive JS/test --compilers coffee:coffee-script/register",
    "test-watch": "mocha --recursive JS/test --compilers coffee:coffee-script/register --watch test"
  },
  "dependencies": {
    "array-includes": "^3.0.2",
    "bluebird": "^3.4.6",
    "browserify": "^10.2.1",
    "coffee-script": "^1.11.1",
    "coffeeify": "^1.1.0",
    "d3": "^3.4.0",
    "debug": "~2.2.0",
    "domready": "^1.0.8",
    "dotenv": "^2.0.0",
    "envify": "^3.4.1",
    "grunt": "^0.4.5",
    "jsdom": "^9.6.0",
    "lodash": "^4.5.1",
    "mocha": "^3.1.2",
    "mustache": "^2.2.1",
    "phantomjs-prebuilt": "^2.1.13",
    "query-string": "^4.0.2",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "should": "^11.1.1",
    "stringify": "^5.1.0",
    "watchify": "^3.2.1",
    "webdriverio": "^4.2.16",
    "winston": "^2.3.0",
    "winston-daily-rotate-file": "^1.3.1"
  },
  "author": "Stephanie Sachrajda, Patrick King, Alaa Azazi",
  "devDependencies": {
    "convert-source-map": "^1.2.0",
    "express": "^4.13.4",
    "express-generator": "^4.13.1",
    "fs-extra": "^0.28.0",
    "markdown-styles": "^3.1.5",
    "mustache-express": "^1.2.2",
    "uglifyify": "^3.0.1",
    "zip-folder": "^1.0.0"
  },
  "license": "SEE LICENSE IN LICENSE.txt"
}
