    "webpack-cli": "3.3.10",
    "webpack-dev-middleware": "3.7.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "eslint-junit": {
    "suiteName": "ESLint",
    "output": "./gl-junit.xml"
  },
  "jest-junit": {
    "suiteName": "Jest",
    "outputName": "gl-junit.xml"
  },
  "browserslist": [
    "last 2 version",
    "> 1%",
    "not dead",
    "ie 11"
  ]
}
