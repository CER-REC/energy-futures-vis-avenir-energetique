{
  "name": "d3-neb",
  "version": "1.3.2",
  "description": "CER project",
  "scripts": {
    "build": "browserify JS/App.coffee -p ./tasks/prod-dotenv-browserify-plugin.js -t coffeeify --extension='.coffee' -t [ stringify --extensions [.mustache .css] ] -t envify -g uglifyify -o dist/public/bundle.js",
    "builddev": "browserify JS/App.coffee -p ./tasks/dev-dotenv-browserify-plugin.js -t coffeeify --extension='.coffee' -t [ stringify --extensions [.mustache .css] ] -t envify -o public/bundle.js --debug --verbose",
    "watch": "watchify JS/App.coffee -p ./tasks/dev-dotenv-browserify-plugin.js -t coffeeify --extension='.coffee' -t [ stringify --extensions [.mustache .css] ] -t envify -o public/bundle.js --verbose",
    "distribute": "node ./tasks/distribute.js",
    "start": "node ./JS/servers/DevelopmentServer/DevelopmentServer.js",
    "debug": "nodemon --debug ./JS/servers/DevelopmentServer/DevelopmentServer.js",
    "start-production": "nodemon ./JS/servers/ProductionServer/ProductionServer.js",
    "ingest": "node ./JS/validation/Ingest.js",
    "test": "mocha --recursive JS/test/**/*.coffee --require coffeescript/register --exit",
    "test-watch": "mocha --recursive JS/test/**/*.coffee --require coffeescript/register --watch test",
    "visual-studio-install": "node ./tasks/visual-studio-install.js",
    "clean-vs": "node tasks/clean-vs.js"
  },
  "dependencies": {
    "array-includes": "^3.0.3",
    "array.prototype.find": "^2.1.0",
    "bluebird": "^3.7.1",
    "coffee-script": "1.12.5",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mustache": "^3.1.0",
    "mustache-express": "^1.3.0",
    "query-string": "^6.8.3",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "winston": "^3.2.1",
    "d3": "3.5.17",
    "lodash": "^4.17.15",
    "winston-daily-rotate-file": "^4.2.1"
  },
  "author": "Stephanie Sachrajda, Patrick King, Alaa Azazi",
  "devDependencies": {
    "d3-ease": "^1.0.5",
    "d3-path": "^1.0.8",
    "browserify": "^16.5.0",
    "browser-cookies": "^1.2.0",
    "coffeeify": "^3.0.1",
    "coffeelint": "^2.1.0",
    "coffeelint-no-explicit-parens": "^1.0.2",
    "coffeelint-no-trailing-commas": "^1.0.4",
    "coffeelint-prefer-fewer-parens": "^0.1.0",
    "coffeescope2": "^0.5.1",
    "convert-source-map": "^1.6.0",
    "debug": "~4.1.1",
    "domready": "^1.0.8",
    "envify": "^4.1.0",
    "fs-extra": "^8.1.0",
    "mocha": "^6.2.2",
    "nodemon": "^1.19.4",
    "should": "^13.2.3",
    "stringify": "^5.2.0",
    "uglifyify": "^5.0.2",
    "watchify": "^3.11.1"
  },
  "license": "SEE LICENSE IN LICENSE.txt"
}
